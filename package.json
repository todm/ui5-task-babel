{
  "name": "@todms/ui5-task-babel",
  "version": "1.0.3",
  "description": "UI5 task and middleware for transpiling next-gen js into backwards compatible code using BabelJS",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@ui5/logger": "^2.0.1",
    "micromatch": "^4.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@ui5/cli": "^2.10.3",
    "eslint": "^8.8.0",
    "typescript": "^4.5.5",
    "ui5-tooling-types": "github:todm/ui5-tooling-types"
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "ui5 build --config ./test/ui5-test.yaml --dest ./test/dist --clean-dest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/todm/ui5-task-babel.git"
  },
  "keywords": [
    "ui5",
    "babel",
    "task",
    "middleware",
    "typescript",
    "js"
  ],
  "author": "todm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/todm/ui5-task-babel/issues"
  },
  "homepage": "https://github.com/todm/ui5-task-babel#readme",
  "files": [
    "ui5.yaml",
    "lib/*/**.*"
  ]
}
